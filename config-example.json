        {
            "name": "wpseo_test",
            "action": "faker",
            "faker_type": "sentence"
        }
    ],
    "_custom_tables": [
        {
            "name": "wp_yoast_seo_links",
            "primary_key": "id",
            "columns": [
                {
                    "name": "url",
                    "action": "faker",
                    "faker_type": "url"
                }
            ]
        }
