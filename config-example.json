{
    "post_types": [
        {
            "name": "post",
            "fields": [
                {
                    "name": "post_title",
                    "action": "faker",
                    "faker_type": "sentence"
                }
            ],
            "meta_fields": [
                {
                    "key": "_edit_lock",
                    "action": "remove"
                },
                {
                    "key": "_edit_last",
