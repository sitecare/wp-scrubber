                }
            ],
            "meta_fields": [
                {
                    "key": "committee_url",
                    "action": "remove"
                }
            ]
        }
    ],
    "user_data": {
        "fields": [
            {
                "name": "display_name",
                "action": "faker",
                "faker_type": "name"
            },
            {
                "name": "user_email",
